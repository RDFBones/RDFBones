
@prefix sw:  <http://softwareOntology.com/>  .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

sw:skeletalInventoryPage 	sw:pageElement  sw:boneContainer1 .

sw:boneContainer1		rdf:type		sw:TabContainer ;
						sw:tab			sw:singleBonesTab ;
						sw:tab			sw:coherentBonesTab .

# Tab Definition

sw:singleBonesTab		rdf:type 		sw:Tab ;
						sw:tabTitle		"Single Bones" ;
						sw:contains		sw:singleBonesContainer .

sw:coherentBonesTab		rdf:type 		sw:Tab ;
						sw:tabTitle		"Coherent Bones " ;
						sw:contains		sw:coherentBonesContainer .

### DataTable

sw:singleBonesContainer	rdf:type		sw:DataContainer ;
						sw:dataKey		"singleBones" ;
						sw:addText 		"Add new single bone" .

### LiteralField

sw:singleBonesContainer		sw:dataField	sw:typeDataField1 .

sw:typeDataField1		rdf:type 		sw:LiteralField ;
						sw:num			"1" ;
						sw:dataKey		"type" ;
						sw:title   		"Type" .

### LabelField

sw:singleBonesContainer		sw:dataField	sw:labelDataField1 .

sw:labelDataField1			rdf:type 		sw:LiteralField ;
							sw:num			"2" ;
							sw:dataKey		"label" ;
							sw:title        "Label" .

### CounterField

sw:singleBonesContainer		sw:dataField	sw:counterDataField1 .

sw:counterDataField1	rdf:type 		sw:LiteralField ;
						sw:num			"3" ;
						sw:dataKey		"boneSegments" ;
						sw:title		"Bone Segments " .

### EditButton

sw:singleBonesContainer		sw:dataField	sw:editSingleButton .
sw:editSingleButton			rdf:type		sw:EditButton ;
							sw:num			"5" .
							
							
							
sw:editSingleButton			sw:input		sw:dataInput1 .
sw:dataInput1				rdf:type		sw:global .
sw:dataInput1				sw:dataKey		"individual" .

sw:editSingleButton			sw:input		sw:dataInput2 .
sw:dataInput2				rdf:type		sw:local .
sw:dataInput2				sw:dataKey		"boneUri" .
