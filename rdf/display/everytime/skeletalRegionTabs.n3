@prefix sw:  <http://softwareOntology.com/>  .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

sw:coherentBonesTab    sw:contains		sw:skeletalRegionTab .


sw:skeletalRegionTab    rdf:type		sw:TabContainer ;
						sw:tab			sw:c_SkullTab ;
						sw:tab			sw:c_shoulderTab ;
						sw:tab			sw:c_vertebralColumnTab ;
						sw:tab			sw:c_thoraricBonesTab ;
						sw:tab			sw:c_pelvicGridleTab ;
						sw:tab			sw:c_upperLimbsTab ;
						sw:tab			sw:c_lowerLimbsTab .

sw:c_skullTab			rdf:type 			sw:Tab ;
						sw:tabTitle			"Skull" ;
						sw:number			"1" .
						# sw:contains		sw:singleBonesContainer .

sw:c_shoulderTab		rdf:type 			sw:Tab ;
						sw:tabTitle			"Shoulder" ;
						sw:number			"2" .
						# sw:contains		sw:coherentBonesContainer .

sw:c_vertebralColumnTab			rdf:type 			sw:Tab ;
						sw:tabTitle			"Vertebral Column" ;
						sw:number			"3" .
						# sw:contains		sw:singleBonesContainer .

sw:c_thoraricBonesTab		rdf:type 			sw:Tab ;
						sw:tabTitle			"Thoracic Bones" ;
						sw:number			"4" .
						# sw:contains		sw:coherentBonesContainer .
						
sw:c_pelvicGridleTab			rdf:type 			sw:Tab ;
						sw:tabTitle			"Pelvic Gridle" ;
						sw:number			"5" .
						# sw:contains		sw:singleBonesContainer .

sw:c_upperLimbsTab		rdf:type 			sw:Tab ;
						sw:tabTitle			"Upper Limbs" ;
						sw:number			"6" .
						# sw:contains		sw:coherentBonesContainer .
			
sw:c_lowerLimbsTab		rdf:type	 		sw:Tab ;
						sw:tabTitle			"Lower Limbs" ;
						sw:number			"7" .
						# sw:contains		sw:singleBonesContainer .

					